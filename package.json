{
  "name": "landing-kit",
  "version": "1.0.2",
  "private": true,
  "description": "Landing Kit",
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=22.19.0",
    "npm": ">=10.8.2"
  },
  "scripts": {
    "dev": "npm run dev -w @landing-kit/next",
    "build": "npm run build -w @landing-kit/next",
    "lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "publish:all": "npm publish -w @landing-kit/core --access public && npm publish -w @landing-kit/templates --access public && npm publish -w @landing-kit/next --access public && npm publish -w create-landing-kit --access public",
    "bump": "bash scripts/bump-all.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enxtur/landing-kit.git"
  },
  "keywords": [
    "landing",
    "kit",
    "landing-kit"
  ],
  "author": "enxtur",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enxtur/landing-kit/issues"
  },
  "homepage": "https://github.com/enxtur/landing-kit#readme",
  "dependencies": {
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/node": "^22.19.11",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  }
}
