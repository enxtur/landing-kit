{
  "name": "landing-kit",
  "version": "0.0.1",
  "private": true,
  "description": "Landing Kit",
  "main": "index.js",
  "workspaces": ["packages/*", "examples/*"],
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "dev": "npm run dev -w @landing-kit/next",
    "build": "npm run build -w @landing-kit/next",
    "lint": "npm run lint -w @landing-kit/next",
    "test": "echo \"Error: no test specified\" && exit 1",
    "publish:all": "npm publish -w @landing-kit/core --access public && npm publish -w @landing-kit/templates --access public && npm publish -w @landing-kit/next --access public && npm publish -w create-landing-kit --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enxtur/landing-kit.git"
  },
  "keywords": ["landing", "kit", "landing-kit"],
  "author": "enxtur",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enxtur/landing-kit/issues"
  },
  "homepage": "https://github.com/enxtur/landing-kit#readme",
  "dependencies": {
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@types/node": "^25.2.1",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "typescript": "^5.9.3"
  }
}
